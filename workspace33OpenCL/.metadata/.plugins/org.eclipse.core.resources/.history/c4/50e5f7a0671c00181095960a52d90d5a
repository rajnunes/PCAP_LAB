__kernel void row_powers(__global int* As, __global int* Bs, int n, int m)
{
	int i = get_global_id(0);
	int j = get_global_id(1);
	
	int x = i * m + j;
	if(i == 0 || j == 0 || i == n - 1 || j == m - 1)
		Bs[x] = As[x];
	else
	{
		int temp = As[x];
		Bs[x] = 0;
		int d = 1;
		while(temp != 0)
		{
			Bs[x] += d * (1 - temp % 2);
			temp /= 2;
			d *= 10;
		}
	}
}